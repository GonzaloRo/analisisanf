{{> navigation }}
<div id="content-wrapper">
        <div class="container-fluid">
                <!-- Breadcrumbs-->
                <ol class="breadcrumb">
                <li class="breadcrumb-item">
                        <a href="/contabilidad_general/estados_financieros/listado">
                        Estados Financieros
                        </a> 
                </li>
                <li class="breadcrumb-item active">Estado de Capital</li>
                </ol>
                <div class="row">
                        <div class="col-md-7 mx-auto">
                        <div class="card">
                        <div class="card-body">
                                <div class="table-responsive">
                                        <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                                                <thead>
                                                    <tr>
                                                        <th colspan="6" class="text-center">

                                                            PROPLASTIC S.A. DE C.V.
                                                            <br>
                                                            Estado de Capital
                                                            <br>

                                                            Del {{periodocontable.FECHAINICIO}} al {{periodocontable.FECHAFINAL}} de {{periodocontable.MES}} de {{periodocontable.ANIO}} </td>

                                                        </th>
                                                    </tr>
                                                </thead>
                                                    <tr>

                                                        <th class="text-center">CODIGO</th>
                                                        <th class="text-center">CUENTAS</th>
                                                        <th class="text-center">SALDO </th>
                                                    </tr>
                                                    {{#each cuentas_patrimonio}}
                                                    <tbody>
                                                        <tr id="ID_ESTADOFINANCIERO">
                                                                <td width="15%">{{ CODIGO_CUENTA}}</td>
                                                                <td>{{ NOMBRE_CUENTA }}</td>
                                                                <td class="text-center">{{ SALDO_CUENTA }}</td>                    
                                                        </tr>
                                                    </tbody>
                                                    {{/each}}
                                                     <tr>
                                                        <th class="text-center"</th>
                                                        <th class="text-right">CAPITAL SOCIAL</th>
                                                        <th class="text-center">{{total}}</th>
                                                    </tr>

                                        </table>
                                </div>
                        </div>
                        </div>
                        </div>
                </div>
        </div><!-- /.container-fluid -->
</div><!-- /.content-wrapper -->
{{> footer }}

